{
  "name": "natserract",
  "version": "0.1.0",
  "scripts": {
    "build:prod": "bsb -make-world && npm run clean && webpack --mode production",
    "build:dev": "bsb -make-world && npm run clean && webpack --mode development",
    "start": "bsb -make-world -w -ws _ && bsb -clean-world",
    "serve:prod": "webpack-dev-server --open --mode production",
    "start:dev": "webpack-dev-server --open --mode development",
    "start:prod": "npm run build:prod && npm run serve:prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "homepage": "http://natserract.github.io/natserract",
    "clean": "rm -rf public",
    "dev": "webpack-dev-server",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d public"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bs-css": "^11.0.0",
    "markdown-loader": "^8.0.0",
    "marked": "^1.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^7.2.0",
    "reason-react": ">=0.7.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.5",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "babel-loader": "^8.3.0",
    "bs-platform": "^6.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^5.5.0",
    "moduleserve": "^0.8.4",
    "transfer-webpack-plugin": "^0.1.4",
    "ts-loader": "^9.4.2",
    "typescript": "^3.8.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3"
  }
}
